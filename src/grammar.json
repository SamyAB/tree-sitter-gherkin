{
  "name": "gherkin",
  "rules": {
    "feature": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "feature_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "title"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "scenario"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "multiline_text"
                },
                {
                  "type": "SYMBOL",
                  "name": "scenario"
                }
              ]
            }
          ]
        }
      ]
    },
    "given_keyword": {
      "type": "STRING",
      "value": "    Given "
    },
    "when_keyword": {
      "type": "STRING",
      "value": "    When "
    },
    "then_keyword": {
      "type": "STRING",
      "value": "    Then "
    },
    "feature_keyword": {
      "type": "STRING",
      "value": "Feature: "
    },
    "title": {
      "type": "PATTERN",
      "value": "[A-Z][a-zA-Z ]+\\n"
    },
    "multiline_text": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\\n]"
              },
              {
                "type": "STRING",
                "value": "\n"
              }
            ]
          }
        }
      ]
    },
    "scenario": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "scenario_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "title"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "steps"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "multiline_text"
                },
                {
                  "type": "SYMBOL",
                  "name": "steps"
                }
              ]
            }
          ]
        }
      ]
    },
    "scenario_keyword": {
      "type": "STRING",
      "value": "  Scenario: "
    },
    "steps": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "given_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "step_definition"
        },
        {
          "type": "SYMBOL",
          "name": "when_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "step_definition"
        },
        {
          "type": "SYMBOL",
          "name": "then_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "step_definition"
        }
      ]
    },
    "step_definition": {
      "type": "PATTERN",
      "value": "[^A-Z][a-z][a-z ]+\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

