{
  "name": "gherkin",
  "rules": {
    "feature": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "feature_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "title"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "description"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "scenarios"
        }
      ]
    },
    "feature_keyword": {
      "type": "STRING",
      "value": "Feature: "
    },
    "title": {
      "type": "PATTERN",
      "value": "[A-Z][a-zA-Z ]+\\n"
    },
    "description": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\\n]"
              },
              {
                "type": "STRING",
                "value": "\n"
              }
            ]
          }
        }
      ]
    },
    "scenarios": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "scenario"
      }
    },
    "scenario": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "scenario_keyword"
            },
            {
              "type": "SYMBOL",
              "name": "example_keyword"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "title"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "description"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "steps"
        }
      ]
    },
    "scenario_keyword": {
      "type": "STRING",
      "value": "  Scenario: "
    },
    "example_keyword": {
      "type": "STRING",
      "value": "  Example: "
    },
    "steps": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "full_step"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "full_step"
          }
        }
      ]
    },
    "full_step": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "step_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "step_definition"
        }
      ]
    },
    "step_keyword": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "given_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "when_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "then_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "and_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "but_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "asterisk_keyword"
        }
      ]
    },
    "given_keyword": {
      "type": "STRING",
      "value": "    Given "
    },
    "when_keyword": {
      "type": "STRING",
      "value": "    When "
    },
    "then_keyword": {
      "type": "STRING",
      "value": "    Then "
    },
    "and_keyword": {
      "type": "STRING",
      "value": "    And "
    },
    "but_keyword": {
      "type": "STRING",
      "value": "    But "
    },
    "asterisk_keyword": {
      "type": "STRING",
      "value": "    * "
    },
    "step_definition": {
      "type": "PATTERN",
      "value": "[^A-Z][a-z][a-z ]+\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

